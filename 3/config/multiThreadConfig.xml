<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!-- 
    Количество потоков, 
    1, 2, 3 и т.д.
    0 - автоматический подбор 
    -->
    <numberOfThreads>12</numberOfThreads>
     
    <!-- Количество повторов для каждого теста -->
    <numberOfTests>5</numberOfTests>

    <!-- Размерность вектора -->
    <vectorSize>100000000</vectorSize>

    <!-- Путь для сохранения времени теста -->
    <fileForTestTime>multi_thread_test_time_log.txt</fileForTestTime>
    
    <!-- Путь для сохранения метрик теста -->
    <fileForTestMetrics>multi_thread_test_metrics_log.txt</fileForTestMetrics>
</config>