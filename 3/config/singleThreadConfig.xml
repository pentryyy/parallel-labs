<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!-- 
    Количество потоков, 
    используется только для параллельных методов 
    -->
    <numberOfThreads>20</numberOfThreads>
     
    <!-- Количество повторов для каждого теста -->
    <numberOfTests>5</numberOfTests>

    <!-- Размерность вектора -->
    <vectorSize>100000000</vectorSize>

    <!-- Путь для сохранения времени теста -->
    <fileForTestTime>single_thread_test_time_log.txt</fileForTestTime>
    
    <!-- Путь для сохранения метрик теста -->
    <fileForTestMetrics>single_thread_test_metrics_log.txt</fileForTestMetrics>
</config>